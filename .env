# LeHiboo Environment Variables - Default (Development)
# This file is used as the default when no specific environment is specified.
ENVIRONMENT=development

# API Configuration
API_BASE_URL=https://preprod.lehiboo.com/wp-json/lehiboo/v2
AI_BASE_URL=https://preprod.lehiboo.com/api-planner
API_KEY=

# Website URLs
WEBSITE_URL=https://lehiboo.fr
PRIVACY_POLICY_URL=https://lehiboo.fr/privacy
TERMS_OF_SERVICE_URL=https://lehiboo.fr/terms

# Firebase Configuration
FIREBASE_PROJECT_ID=lehiboo-app
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:android:abcdef

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Feature Flags
ANALYTICS_ENABLED=true
CRASHLYTICS_ENABLED=true